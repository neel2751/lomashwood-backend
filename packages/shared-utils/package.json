{
  "name": "@lomash-wood/shared-utils",
  "version": "1.0.0",
  "description": "Shared utility functions for Lomash Wood microservices",
  "license": "UNLICENSED",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./logger": {
      "import": "./dist/logger.js",
      "require": "./dist/logger.js",
      "types": "./dist/logger.d.ts"
    },
    "./crypto": {
      "import": "./dist/crypto.js",
      "require": "./dist/crypto.js",
      "types": "./dist/crypto.d.ts"
    },
    "./date": {
      "import": "./dist/date.js",
      "require": "./dist/date.js",
      "types": "./dist/date.d.ts"
    },
    "./pagination": {
      "import": "./dist/pagination.js",
      "require": "./dist/pagination.js",
      "types": "./dist/pagination.d.ts"
    },
    "./retry": {
      "import": "./dist/retry.js",
      "require": "./dist/retry.js",
      "types": "./dist/retry.d.ts"
    },
    "./env": {
      "import": "./dist/env.js",
      "require": "./dist/env.js",
      "types": "./dist/env.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "jest --config jest.config.ts",
    "clean": "rm -rf dist .tsbuildinfo"
  },
  "dependencies": {
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0"
  },
  "devDependencies": {
    "@lomash-wood/tsconfig": "workspace:*",
    "@types/node": "^20.0.0",
    "typescript": "^5.4.0"
  },
  "keywords": [
    "utils",
    "utilities",
    "logger",
    "lomash-wood"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "restricted"
  }
}