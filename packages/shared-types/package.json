{
  "name": "@lomash-wood/shared-types",
  "version": "1.0.0",
  "description": "Shared TypeScript type definitions for Lomash Wood microservices",
  "license": "UNLICENSED",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./auth": {
      "import": "./dist/auth.types.js",
      "require": "./dist/auth.types.js",
      "types": "./dist/auth.types.d.ts"
    },
    "./product": {
      "import": "./dist/product.types.js",
      "require": "./dist/product.types.js",
      "types": "./dist/product.types.d.ts"
    },
    "./order": {
      "import": "./dist/order.types.js",
      "require": "./dist/order.types.js",
      "types": "./dist/order.types.d.ts"
    },
    "./booking": {
      "import": "./dist/booking.types.js",
      "require": "./dist/booking.types.js",
      "types": "./dist/booking.types.d.ts"
    },
    "./content": {
      "import": "./dist/content.types.js",
      "require": "./dist/content.types.js",
      "types": "./dist/content.types.d.ts"
    },
    "./user": {
      "import": "./dist/user.types.js",
      "require": "./dist/user.types.js",
      "types": "./dist/user.types.d.ts"
    },
    "./notification": {
      "import": "./dist/notification.types.js",
      "require": "./dist/notification.types.js",
      "types": "./dist/notification.types.d.ts"
    },
    "./analytics": {
      "import": "./dist/analytics.types.js",
      "require": "./dist/analytics.types.js",
      "types": "./dist/analytics.types.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "clean": "rm -rf dist .tsbuildinfo"
  },
  "devDependencies": {
    "@lomash-wood/tsconfig": "workspace:*",
    "typescript": "^5.4.0"
  },
  "keywords": [
    "types",
    "typescript",
    "lomash-wood"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "restricted"
  }
}