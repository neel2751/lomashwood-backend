{
  "__inputs": [{ "name": "DS_PROMETHEUS", "label": "Prometheus", "type": "datasource", "pluginId": "prometheus" }],
  "title": "Lomash Wood â€” Analytics Service",
  "uid": "lw-analytics",
  "tags": ["lomash-wood", "analytics-service"],
  "timezone": "browser",
  "schemaVersion": 38,
  "refresh": "60s",
  "time": { "from": "now-1h", "to": "now" },
  "panels": [
    {
      "id": 1, "type": "stat", "title": "Events Tracked (1h)",
      "gridPos": { "x": 0, "y": 0, "w": 4, "h": 4 },
      "targets": [{ "expr": "sum(increase(analytics_events_tracked_total{service=\"analytics-service\"}[1h]))", "legendFormat": "Events" }],
      "fieldConfig": { "defaults": { "thresholds": { "steps": [{ "color": "green", "value": null }] } } }
    },
    {
      "id": 2, "type": "stat", "title": "Active Sessions",
      "gridPos": { "x": 4, "y": 0, "w": 4, "h": 4 },
      "targets": [{ "expr": "sum(analytics_active_sessions{service=\"analytics-service\"})", "legendFormat": "Sessions" }],
      "fieldConfig": { "defaults": { "thresholds": { "steps": [{ "color": "green", "value": null }] } } }
    },
    {
      "id": 3, "type": "stat", "title": "Event Ingestion Rate",
      "gridPos": { "x": 8, "y": 0, "w": 4, "h": 4 },
      "targets": [{ "expr": "sum(rate(analytics_events_tracked_total{service=\"analytics-service\"}[2m]))", "legendFormat": "Events/s" }],
      "fieldConfig": { "defaults": { "unit": "reqps" } }
    },
    {
      "id": 4, "type": "stat", "title": "Reports Generated (24h)",
      "gridPos": { "x": 12, "y": 0, "w": 4, "h": 4 },
      "targets": [{ "expr": "sum(increase(analytics_reports_generated_total{service=\"analytics-service\"}[24h]))", "legendFormat": "Reports" }],
      "fieldConfig": { "defaults": { "thresholds": { "steps": [{ "color": "green", "value": null }] } } }
    },
    {
      "id": 5, "type": "stat", "title": "Funnel Completions (24h)",
      "gridPos": { "x": 16, "y": 0, "w": 4, "h": 4 },
      "targets": [{ "expr": "sum(increase(analytics_funnel_completed_total{service=\"analytics-service\"}[24h]))", "legendFormat": "Completions" }],
      "fieldConfig": { "defaults": { "thresholds": { "steps": [{ "color": "green", "value": null }] } } }
    },
    {
      "id": 6, "type": "stat", "title": "Analytics Service Error Rate",
      "gridPos": { "x": 20, "y": 0, "w": 4, "h": 4 },
      "targets": [{ "expr": "100 * sum(rate(http_requests_total{service=\"analytics-service\",status_code=~\"5..\"}[5m])) / sum(rate(http_requests_total{service=\"analytics-service\"}[5m]))", "legendFormat": "Error %" }],
      "fieldConfig": { "defaults": { "unit": "percent", "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 1 }, { "color": "red", "value": 5 }] } } }
    },
    {
      "id": 7, "type": "timeseries", "title": "Event Ingestion Rate by Type",
      "gridPos": { "x": 0, "y": 4, "w": 12, "h": 8 },
      "targets": [{ "expr": "sum(rate(analytics_events_tracked_total{service=\"analytics-service\"}[2m])) by (event_type)", "legendFormat": "{{ event_type }}" }],
      "fieldConfig": { "defaults": { "unit": "reqps" } }
    },
    {
      "id": 8, "type": "timeseries", "title": "Dashboard Query Latency (P99)",
      "gridPos": { "x": 12, "y": 4, "w": 12, "h": 8 },
      "targets": [
        { "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{service=\"analytics-service\"}[2m])) by (le, route))", "legendFormat": "{{ route }} P99" }
      ],
      "fieldConfig": { "defaults": { "unit": "s" } }
    },
    {
      "id": 9, "type": "timeseries", "title": "Pipeline Processing Time",
      "gridPos": { "x": 0, "y": 12, "w": 24, "h": 8 },
      "targets": [
        { "expr": "histogram_quantile(0.99, sum(rate(analytics_pipeline_duration_seconds_bucket{service=\"analytics-service\"}[5m])) by (le, pipeline))", "legendFormat": "{{ pipeline }} P99" }
      ],
      "fieldConfig": { "defaults": { "unit": "s" } }
    }
  ]
}