{
  "__inputs": [{ "name": "DS_PROMETHEUS", "label": "Prometheus", "type": "datasource", "pluginId": "prometheus" }],
  "title": "Lomash Wood â€” Redis Cache",
  "uid": "lw-redis",
  "tags": ["lomash-wood", "redis", "cache"],
  "timezone": "browser",
  "schemaVersion": 38,
  "refresh": "30s",
  "time": { "from": "now-1h", "to": "now" },
  "panels": [
    {
      "id": 1, "type": "stat", "title": "Cache Hit Ratio %",
      "gridPos": { "x": 0, "y": 0, "w": 4, "h": 4 },
      "targets": [{ "expr": "100 * sum(rate(redis_keyspace_hits_total[5m])) / (sum(rate(redis_keyspace_hits_total[5m])) + sum(rate(redis_keyspace_misses_total[5m])))", "legendFormat": "Hit Ratio %" }],
      "fieldConfig": { "defaults": { "unit": "percent", "thresholds": { "steps": [{ "color": "red", "value": null }, { "color": "yellow", "value": 80 }, { "color": "green", "value": 95 }] } } }
    },
    {
      "id": 2, "type": "stat", "title": "Connected Clients",
      "gridPos": { "x": 4, "y": 0, "w": 4, "h": 4 },
      "targets": [{ "expr": "redis_connected_clients", "legendFormat": "Clients" }],
      "fieldConfig": { "defaults": { "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 100 }, { "color": "red", "value": 500 }] } } }
    },
    {
      "id": 3, "type": "stat", "title": "Memory Used",
      "gridPos": { "x": 8, "y": 0, "w": 4, "h": 4 },
      "targets": [{ "expr": "redis_memory_used_bytes", "legendFormat": "Memory" }],
      "fieldConfig": { "defaults": { "unit": "bytes" } }
    },
    {
      "id": 4, "type": "stat", "title": "Total Keys",
      "gridPos": { "x": 12, "y": 0, "w": 4, "h": 4 },
      "targets": [{ "expr": "sum(redis_db_keys)", "legendFormat": "Keys" }],
      "fieldConfig": { "defaults": { "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 1000000 }] } } }
    },
    {
      "id": 5, "type": "stat", "title": "Commands/s",
      "gridPos": { "x": 16, "y": 0, "w": 4, "h": 4 },
      "targets": [{ "expr": "rate(redis_commands_processed_total[2m])", "legendFormat": "Cmds/s" }],
      "fieldConfig": { "defaults": { "unit": "reqps" } }
    },
    {
      "id": 6, "type": "stat", "title": "Expired Keys (5m)",
      "gridPos": { "x": 20, "y": 0, "w": 4, "h": 4 },
      "targets": [{ "expr": "increase(redis_expired_keys_total[5m])", "legendFormat": "Expired" }],
      "fieldConfig": { "defaults": { "thresholds": { "steps": [{ "color": "green", "value": null }] } } }
    },
    {
      "id": 7, "type": "timeseries", "title": "Cache Hits vs Misses",
      "gridPos": { "x": 0, "y": 4, "w": 12, "h": 8 },
      "targets": [
        { "expr": "rate(redis_keyspace_hits_total[2m])", "legendFormat": "Hits" },
        { "expr": "rate(redis_keyspace_misses_total[2m])", "legendFormat": "Misses" }
      ],
      "fieldConfig": { "defaults": { "unit": "reqps" },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Hits" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "Misses" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "id": 8, "type": "timeseries", "title": "Memory Usage Over Time",
      "gridPos": { "x": 12, "y": 4, "w": 12, "h": 8 },
      "targets": [
        { "expr": "redis_memory_used_bytes", "legendFormat": "Used" },
        { "expr": "redis_memory_max_bytes", "legendFormat": "Max" },
        { "expr": "redis_memory_used_rss_bytes", "legendFormat": "RSS" }
      ],
      "fieldConfig": { "defaults": { "unit": "bytes" } }
    },
    {
      "id": 9, "type": "timeseries", "title": "Command Rate by Type",
      "gridPos": { "x": 0, "y": 12, "w": 12, "h": 8 },
      "targets": [{ "expr": "rate(redis_commands_processed_total[2m])", "legendFormat": "Commands/s" }],
      "fieldConfig": { "defaults": { "unit": "reqps" } }
    },
    {
      "id": 10, "type": "timeseries", "title": "Network I/O",
      "gridPos": { "x": 12, "y": 12, "w": 12, "h": 8 },
      "targets": [
        { "expr": "rate(redis_net_input_bytes_total[2m])", "legendFormat": "Input" },
        { "expr": "rate(redis_net_output_bytes_total[2m])", "legendFormat": "Output" }
      ],
      "fieldConfig": { "defaults": { "unit": "binBps" } }
    }
  ]
}