{
  "__inputs": [{ "name": "DS_PROMETHEUS", "label": "Prometheus", "type": "datasource", "pluginId": "prometheus" }],
  "title": "Lomash Wood — Latency",
  "uid": "lw-latency",
  "tags": ["lomash-wood", "latency", "performance"],
  "timezone": "browser",
  "schemaVersion": 38,
  "refresh": "30s",
  "time": { "from": "now-1h", "to": "now" },
  "panels": [
    {
      "id": 1, "type": "stat", "title": "Overall P50 Latency",
      "gridPos": { "x": 0, "y": 0, "w": 4, "h": 4 },
      "targets": [{ "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))", "legendFormat": "P50" }],
      "fieldConfig": { "defaults": { "unit": "s", "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 0.5 }, { "color": "red", "value": 1 }] } } }
    },
    {
      "id": 2, "type": "stat", "title": "Overall P95 Latency",
      "gridPos": { "x": 4, "y": 0, "w": 4, "h": 4 },
      "targets": [{ "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))", "legendFormat": "P95" }],
      "fieldConfig": { "defaults": { "unit": "s", "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 1 }, { "color": "red", "value": 2 }] } } }
    },
    {
      "id": 3, "type": "stat", "title": "Overall P99 Latency",
      "gridPos": { "x": 8, "y": 0, "w": 4, "h": 4 },
      "targets": [{ "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))", "legendFormat": "P99" }],
      "fieldConfig": { "defaults": { "unit": "s", "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 2 }, { "color": "red", "value": 3 }] } } }
    },
    {
      "id": 4, "type": "stat", "title": "Payment P99 Latency",
      "gridPos": { "x": 12, "y": 0, "w": 4, "h": 4 },
      "targets": [{ "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{service=\"order-payment-service\"}[5m])) by (le))", "legendFormat": "P99" }],
      "fieldConfig": { "defaults": { "unit": "s", "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 3 }, { "color": "red", "value": 5 }] } } }
    },
    {
      "id": 5, "type": "stat", "title": "DB Query P99 Latency",
      "gridPos": { "x": 16, "y": 0, "w": 4, "h": 4 },
      "targets": [{ "expr": "histogram_quantile(0.99, sum(rate(prisma_client_queries_duration_histogram_ms_bucket[5m])) by (le))", "legendFormat": "P99" }],
      "fieldConfig": { "defaults": { "unit": "ms", "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 200 }, { "color": "red", "value": 1000 }] } } }
    },
    {
      "id": 6, "type": "stat", "title": "Redis P99 Latency",
      "gridPos": { "x": 20, "y": 0, "w": 4, "h": 4 },
      "targets": [{ "expr": "histogram_quantile(0.99, sum(rate(redis_command_duration_seconds_bucket[5m])) by (le))", "legendFormat": "P99" }],
      "fieldConfig": { "defaults": { "unit": "s", "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 0.01 }, { "color": "red", "value": 0.1 }] } } }
    },
    {
      "id": 7, "type": "timeseries", "title": "P50 / P95 / P99 Latency — All Services",
      "gridPos": { "x": 0, "y": 4, "w": 24, "h": 8 },
      "targets": [
        { "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket[2m])) by (le))", "legendFormat": "P50" },
        { "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[2m])) by (le))", "legendFormat": "P95" },
        { "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket[2m])) by (le))", "legendFormat": "P99" }
      ],
      "fieldConfig": { "defaults": { "unit": "s", "custom": { "lineWidth": 2 } },
        "overrides": [
          { "matcher": { "id": "byName", "options": "P99" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "P95" }, "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "P50" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "id": 8, "type": "timeseries", "title": "P99 Latency by Service",
      "gridPos": { "x": 0, "y": 12, "w": 12, "h": 8 },
      "targets": [
        { "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket[2m])) by (le, service))", "legendFormat": "{{ service }}" }
      ],
      "fieldConfig": { "defaults": { "unit": "s" } }
    },
    {
      "id": 9, "type": "timeseries", "title": "P99 Latency by Route (Top 10 Slowest)",
      "gridPos": { "x": 12, "y": 12, "w": 12, "h": 8 },
      "targets": [{ "expr": "topk(10, histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, route, service)))", "legendFormat": "{{ service }} {{ route }}" }],
      "fieldConfig": { "defaults": { "unit": "s" } }
    },
    {
      "id": 10, "type": "heatmap", "title": "Request Duration Heatmap — API Gateway",
      "gridPos": { "x": 0, "y": 20, "w": 12, "h": 8 },
      "targets": [{ "expr": "sum(rate(http_request_duration_seconds_bucket{service=\"api-gateway\"}[2m])) by (le)", "legendFormat": "{{ le }}", "format": "heatmap" }],
      "options": { "yAxis": { "unit": "s" } }
    },
    {
      "id": 11, "type": "heatmap", "title": "Request Duration Heatmap — Payments",
      "gridPos": { "x": 12, "y": 20, "w": 12, "h": 8 },
      "targets": [{ "expr": "sum(rate(http_request_duration_seconds_bucket{service=\"order-payment-service\"}[2m])) by (le)", "legendFormat": "{{ le }}", "format": "heatmap" }],
      "options": { "yAxis": { "unit": "s" } }
    },
    {
      "id": 12, "type": "timeseries", "title": "Database Query Latency by Service (P99)",
      "gridPos": { "x": 0, "y": 28, "w": 12, "h": 8 },
      "targets": [{ "expr": "histogram_quantile(0.99, sum(rate(prisma_client_queries_duration_histogram_ms_bucket[2m])) by (le, service))", "legendFormat": "{{ service }}" }],
      "fieldConfig": { "defaults": { "unit": "ms" } }
    },
    {
      "id": 13, "type": "timeseries", "title": "External Dependency Latency (Stripe, SES, Twilio)",
      "gridPos": { "x": 12, "y": 28, "w": 12, "h": 8 },
      "targets": [
        { "expr": "histogram_quantile(0.99, sum(rate(external_request_duration_seconds_bucket{provider=\"stripe\"}[2m])) by (le))", "legendFormat": "Stripe P99" },
        { "expr": "histogram_quantile(0.99, sum(rate(external_request_duration_seconds_bucket{provider=\"ses\"}[2m])) by (le))", "legendFormat": "AWS SES P99" },
        { "expr": "histogram_quantile(0.99, sum(rate(external_request_duration_seconds_bucket{provider=\"twilio\"}[2m])) by (le))", "legendFormat": "Twilio P99" }
      ],
      "fieldConfig": { "defaults": { "unit": "s" } }
    }
  ]
}