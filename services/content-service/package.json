{
  "name": "@lomash-wood/content-service",
  "version": "1.0.0",
  "description": "Lomash Wood Content Service - CMS, Blog, Media, SEO, Landing Pages",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "start": "node dist/main.js",
    "dev": "nodemon",
    "lint": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "test": "jest --passWithNoTests",
    "test:unit": "jest --testPathPattern=tests/unit --passWithNoTests",
    "test:integration": "jest --testPathPattern=tests/integration --passWithNoTests",
    "test:e2e": "jest --testPathPattern=tests/e2e --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "db:generate": "prisma generate --config prisma.config.ts",
    "db:push": "prisma db push --config prisma.config.ts",
    "db:migrate": "prisma migrate dev --config prisma.config.ts",
    "db:migrate:deploy": "prisma migrate deploy --config prisma.config.ts",
    "db:seed": "ts-node prisma/seed.ts",
    "db:studio": "prisma studio --config prisma.config.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.600.0",
    "@aws-sdk/client-ses": "^3.600.0",
    "@aws-sdk/lib-storage": "^3.600.0",
    "@prisma/adapter-pg": "^7.4.1",
    "@prisma/client": "^7.4.1",
    "axios": "^1.7.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^4.4.0",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.3.1",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.9.3",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "pg": "^8.13.0",
    "pino": "^9.2.0",
    "pino-http": "^10.2.0",
    "pino-pretty": "^11.2.1",
    "slugify": "^1.6.6",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/cron": "^2.4.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.9",
    "@types/pg": "^8.11.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0",
    "nodemon": "^3.1.4",
    "prisma": "^7.4.1",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}