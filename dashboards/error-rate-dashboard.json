    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "green",  "value": null },
            { "color": "yellow", "value": 0.5  },
            { "color": "red",    "value": 1.0  }
          ]},
          "unit": "percent",
          "decimals": 2
        },
        "overrides": []
