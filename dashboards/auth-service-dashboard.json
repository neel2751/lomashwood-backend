{
  "__inputs": [{ "name": "DS_PROMETHEUS", "label": "Prometheus", "type": "datasource", "pluginId": "prometheus", "pluginName": "Prometheus" }],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "" }
  ],
  "annotations": { "list": [{ "builtIn": 1, "datasource": { "type": "grafana", "uid": "-- Grafana --" }, "enable": true, "hide": true, "iconColor": "rgba(0, 211, 255, 1)", "name": "Annotations & Alerts", "type": "dashboard" }] },
  "description": "Lomash Wood â€” Auth Service: logins, registrations, token ops, session health",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    { "collapsed": false, "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 }, "id": 100, "title": "Auth Overview", "type": "row" },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "unit": "reqps" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "id": 1,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Login Rate",
      "type": "stat",
      "targets": [{ "expr": "sum(rate(auth_login_total{job=\"auth-service\"}[5m]))", "legendFormat": "logins/s", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 5 }, { "color": "red", "value": 20 }] }, "unit": "percent" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 },
      "id": 2,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Login Failure Rate %",
      "type": "stat",
      "targets": [{ "expr": "sum(rate(auth_login_failed_total{job=\"auth-service\"}[5m])) / sum(rate(auth_login_total{job=\"auth-service\"}[5m])) * 100", "legendFormat": "fail %", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 },
      "id": 3,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Active Sessions",
      "type": "stat",
      "targets": [{ "expr": "auth_active_sessions_total{job=\"auth-service\"}", "legendFormat": "sessions", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "unit": "reqps" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "id": 4,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Registration Rate",
      "type": "stat",
      "targets": [{ "expr": "sum(rate(auth_registration_total{job=\"auth-service\"}[5m]))", "legendFormat": "reg/s", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "unit": "reqps" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 },
      "id": 5,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Token Refresh Rate",
      "type": "stat",
      "targets": [{ "expr": "sum(rate(auth_token_refresh_total{job=\"auth-service\"}[5m]))", "legendFormat": "refresh/s", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 10 }, { "color": "red", "value": 50 }] }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 },
      "id": 6,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Blacklisted Tokens",
      "type": "stat",
      "targets": [{ "expr": "auth_token_blacklist_size{job=\"auth-service\"}", "legendFormat": "tokens", "refId": "A" }]
    },
    { "collapsed": false, "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 }, "id": 101, "title": "Auth Operations", "type": "row" },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "reqps", "color": { "mode": "palette-classic" } }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "id": 10,
      "options": { "tooltip": { "mode": "multi", "sort": "desc" }, "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "title": "Auth Operation Rates",
      "type": "timeseries",
      "targets": [
        { "expr": "sum(rate(auth_login_total{job=\"auth-service\"}[5m]))", "legendFormat": "Login", "refId": "A" },
        { "expr": "sum(rate(auth_registration_total{job=\"auth-service\"}[5m]))", "legendFormat": "Register", "refId": "B" },
        { "expr": "sum(rate(auth_logout_total{job=\"auth-service\"}[5m]))", "legendFormat": "Logout", "refId": "C" },
        { "expr": "sum(rate(auth_token_refresh_total{job=\"auth-service\"}[5m]))", "legendFormat": "Token Refresh", "refId": "D" },
        { "expr": "sum(rate(auth_password_reset_total{job=\"auth-service\"}[5m]))", "legendFormat": "Password Reset", "refId": "E" }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "reqps", "color": { "mode": "palette-classic" } }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "id": 11,
      "options": { "tooltip": { "mode": "multi", "sort": "desc" }, "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "title": "Auth Failures by Type",
      "type": "timeseries",
      "targets": [
        { "expr": "sum(rate(auth_login_failed_total{job=\"auth-service\"}[5m])) by (reason)", "legendFormat": "Login Fail: {{reason}}", "refId": "A" },
        { "expr": "sum(rate(auth_token_invalid_total{job=\"auth-service\"}[5m]))", "legendFormat": "Invalid Token", "refId": "B" },
        { "expr": "sum(rate(auth_token_expired_total{job=\"auth-service\"}[5m]))", "legendFormat": "Expired Token", "refId": "C" }
      ]
    },
    { "collapsed": false, "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 }, "id": 102, "title": "Security", "type": "row" },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "reqps", "color": { "mode": "fixed", "fixedColor": "red" } }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 },
      "id": 20,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "title": "Brute Force Attempts",
      "type": "timeseries",
      "targets": [{ "expr": "sum(rate(auth_brute_force_attempts_total{job=\"auth-service\"}[5m])) by (ip)", "legendFormat": "IP: {{ip}}", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "ms", "color": { "mode": "palette-classic" } }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 },
      "id": 21,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "title": "Auth Operation Latency (p99)",
      "type": "timeseries",
      "targets": [
        { "expr": "histogram_quantile(0.99, sum(rate(auth_operation_duration_ms_bucket{job=\"auth-service\",operation=\"login\"}[5m])) by (le))", "legendFormat": "Login p99", "refId": "A" },
        { "expr": "histogram_quantile(0.99, sum(rate(auth_operation_duration_ms_bucket{job=\"auth-service\",operation=\"register\"}[5m])) by (le))", "legendFormat": "Register p99", "refId": "B" },
        { "expr": "histogram_quantile(0.99, sum(rate(auth_operation_duration_ms_bucket{job=\"auth-service\",operation=\"token_verify\"}[5m])) by (le))", "legendFormat": "Token Verify p99", "refId": "C" }
      ]
    },
    { "collapsed": false, "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 }, "id": 103, "title": "System Resources", "type": "row" },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "percent", "min": 0, "max": 100 }, "overrides": [] },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 24 },
      "id": 30,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "title": "CPU Usage",
      "type": "timeseries",
      "targets": [{ "expr": "rate(process_cpu_seconds_total{job=\"auth-service\"}[5m]) * 100", "legendFormat": "{{instance}}", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "bytes" }, "overrides": [] },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 24 },
      "id": 31,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "title": "Memory (RSS)",
      "type": "timeseries",
      "targets": [{ "expr": "process_resident_memory_bytes{job=\"auth-service\"}", "legendFormat": "{{instance}}", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 24 },
      "id": 32,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "title": "Event Loop Lag",
      "type": "timeseries",
      "targets": [{ "expr": "nodejs_eventloop_lag_seconds{job=\"auth-service\"} * 1000", "legendFormat": "lag ms {{instance}}", "refId": "A" }]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "tags": ["lomash-wood", "auth-service"],
  "templating": { "list": [{ "current": {}, "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "definition": "label_values(up{job=\"auth-service\"}, instance)", "hide": 0, "includeAll": true, "multi": true, "name": "instance", "options": [], "query": "label_values(up{job=\"auth-service\"}, instance)", "refresh": 2, "type": "query", "label": "Instance" }] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Auth Service",
  "uid": "lomash-auth-service",
  "version": 1,
  "weekStart": ""
}
