{
  "__inputs": [{ "name": "DS_PROMETHEUS", "label": "Prometheus", "type": "datasource", "pluginId": "prometheus", "pluginName": "Prometheus" }],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "" }
  ],
  "annotations": { "list": [{ "builtIn": 1, "datasource": { "type": "grafana", "uid": "-- Grafana --" }, "enable": true, "hide": true, "iconColor": "rgba(0, 211, 255, 1)", "name": "Annotations & Alerts", "type": "dashboard" }] },
  "description": "Lomash Wood â€” Appointment Service: bookings, availability, cancellations, reminders",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    { "collapsed": false, "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 }, "id": 100, "title": "Bookings Overview", "type": "row" },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "id": 1,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Bookings Today",
      "type": "stat",
      "targets": [{ "expr": "sum(increase(bookings_created_total{job=\"appointment-service\"}[24h]))", "legendFormat": "bookings", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 10 }, { "color": "red", "value": 20 }] }, "unit": "percent", "min": 0, "max": 100 }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 },
      "id": 2,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Cancellation Rate %",
      "type": "stat",
      "targets": [{ "expr": "sum(rate(bookings_cancelled_total{job=\"appointment-service\"}[1h])) / sum(rate(bookings_created_total{job=\"appointment-service\"}[1h])) * 100", "legendFormat": "cancel %", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 },
      "id": 3,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Upcoming Bookings",
      "type": "stat",
      "targets": [{ "expr": "appointment_upcoming_bookings_total{job=\"appointment-service\"}", "legendFormat": "upcoming", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "id": 4,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Available Slots",
      "type": "stat",
      "targets": [{ "expr": "appointment_available_slots_total{job=\"appointment-service\"}", "legendFormat": "slots", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 },
      "id": 5,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Reminders Sent (1h)",
      "type": "stat",
      "targets": [{ "expr": "sum(increase(appointment_reminders_sent_total{job=\"appointment-service\"}[1h]))", "legendFormat": "reminders", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 3 }, { "color": "red", "value": 10 }] }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 },
      "id": 6,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Slot Conflicts (1h)",
      "type": "stat",
      "targets": [{ "expr": "sum(increase(appointment_slot_conflicts_total{job=\"appointment-service\"}[1h]))", "legendFormat": "conflicts", "refId": "A" }]
    },
    { "collapsed": false, "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 }, "id": 101, "title": "Booking Trends", "type": "row" },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "reqps", "color": { "mode": "palette-classic" } }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "id": 10,
      "options": { "tooltip": { "mode": "multi", "sort": "desc" }, "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "title": "Booking Operations Rate",
      "type": "timeseries",
      "targets": [
        { "expr": "sum(rate(bookings_created_total{job=\"appointment-service\"}[5m]))", "legendFormat": "Created", "refId": "A" },
        { "expr": "sum(rate(bookings_confirmed_total{job=\"appointment-service\"}[5m]))", "legendFormat": "Confirmed", "refId": "B" },
        { "expr": "sum(rate(bookings_cancelled_total{job=\"appointment-service\"}[5m]))", "legendFormat": "Cancelled", "refId": "C" },
        { "expr": "sum(rate(bookings_rescheduled_total{job=\"appointment-service\"}[5m]))", "legendFormat": "Rescheduled", "refId": "D" }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "reqps", "color": { "mode": "palette-classic" } }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "id": 11,
      "options": { "tooltip": { "mode": "multi", "sort": "desc" }, "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "title": "Bookings by Type",
      "type": "timeseries",
      "targets": [
        { "expr": "sum(rate(bookings_created_total{job=\"appointment-service\"}[5m])) by (appointment_type)", "legendFormat": "{{appointment_type}}", "refId": "A" }
      ]
    },
    { "collapsed": false, "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 }, "id": 102, "title": "API & Latency", "type": "row" },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 5 }, "unit": "ms", "color": { "mode": "palette-classic" } }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 },
      "id": 20,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "title": "API Latency Percentiles",
      "type": "timeseries",
      "targets": [
        { "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_ms_bucket{job=\"appointment-service\"}[5m])) by (le))", "legendFormat": "p50", "refId": "A" },
        { "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_ms_bucket{job=\"appointment-service\"}[5m])) by (le))", "legendFormat": "p95", "refId": "B" },
        { "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_ms_bucket{job=\"appointment-service\"}[5m])) by (le))", "legendFormat": "p99", "refId": "C" }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "reqps", "color": { "mode": "palette-classic" } }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 },
      "id": 21,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "title": "HTTP Request Rate by Status",
      "type": "timeseries",
      "targets": [
        { "expr": "sum(rate(http_requests_total{job=\"appointment-service\"}[5m])) by (status)", "legendFormat": "{{status}}", "refId": "A" }
      ]
    },
    { "collapsed": false, "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 }, "id": 103, "title": "System Resources", "type": "row" },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "percent", "min": 0, "max": 100 }, "overrides": [] },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 24 },
      "id": 30,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "title": "CPU Usage",
      "type": "timeseries",
      "targets": [{ "expr": "rate(process_cpu_seconds_total{job=\"appointment-service\"}[5m]) * 100", "legendFormat": "{{instance}}", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "bytes" }, "overrides": [] },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 24 },
      "id": 31,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "title": "Memory (RSS)",
      "type": "timeseries",
      "targets": [{ "expr": "process_resident_memory_bytes{job=\"appointment-service\"}", "legendFormat": "{{instance}}", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 24 },
      "id": 32,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "title": "Job Queue Depth",
      "type": "timeseries",
      "targets": [
        { "expr": "appointment_job_queue_depth{job=\"appointment-service\"}", "legendFormat": "{{queue_name}}", "refId": "A" }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "tags": ["lomash-wood", "appointment-service"],
  "templating": { "list": [{ "current": {}, "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "definition": "label_values(up{job=\"appointment-service\"}, instance)", "hide": 0, "includeAll": true, "multi": true, "name": "instance", "options": [], "query": "label_values(up{job=\"appointment-service\"}, instance)", "refresh": 2, "type": "query", "label": "Instance" }] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Appointment Service",
  "uid": "lomash-appointment-service",
  "version": 1,
  "weekStart": ""
}
