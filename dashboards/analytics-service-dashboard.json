{
  "__inputs": [{ "name": "DS_PROMETHEUS", "label": "Prometheus", "type": "datasource", "pluginId": "prometheus", "pluginName": "Prometheus" }],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "" }
  ],
  "annotations": { "list": [{ "builtIn": 1, "datasource": { "type": "grafana", "uid": "-- Grafana --" }, "enable": true, "hide": true, "iconColor": "rgba(0, 211, 255, 1)", "name": "Annotations & Alerts", "type": "dashboard" }] },
  "description": "Lomash Wood â€” Analytics Service: event ingestion, funnel computation, dashboard refresh, exports",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    { "collapsed": false, "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 }, "id": 100, "title": "Analytics Overview", "type": "row" },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "unit": "reqps" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "id": 1,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Events Ingested/s",
      "type": "stat",
      "targets": [{ "expr": "sum(rate(analytics_events_ingested_total{job=\"analytics-service\"}[5m]))", "legendFormat": "events/s", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 1 }, { "color": "red", "value": 5 }] }, "unit": "percent" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 },
      "id": 2,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Ingestion Error Rate %",
      "type": "stat",
      "targets": [{ "expr": "sum(rate(analytics_ingestion_errors_total{job=\"analytics-service\"}[5m])) / sum(rate(analytics_events_ingested_total{job=\"analytics-service\"}[5m])) * 100", "legendFormat": "error %", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 },
      "id": 3,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Active Dashboards",
      "type": "stat",
      "targets": [{ "expr": "analytics_active_dashboards_total{job=\"analytics-service\"}", "legendFormat": "dashboards", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "id": 4,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Exports (1h)",
      "type": "stat",
      "targets": [{ "expr": "sum(increase(analytics_exports_total{job=\"analytics-service\"}[1h]))", "legendFormat": "exports", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 10000 }, { "color": "red", "value": 100000 }] }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 },
      "id": 5,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Consumer Lag",
      "type": "stat",
      "targets": [{ "expr": "sum(analytics_event_consumer_lag{job=\"analytics-service\"})", "legendFormat": "lag", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 },
      "id": 6,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Funnels Computed (1h)",
      "type": "stat",
      "targets": [{ "expr": "sum(increase(analytics_funnels_computed_total{job=\"analytics-service\"}[1h]))", "legendFormat": "funnels", "refId": "A" }]
    },
    { "collapsed": false, "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 }, "id": 101, "title": "Ingestion Pipeline", "type": "row" },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "reqps", "color": { "mode": "palette-classic" } }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "id": 10,
      "options": { "tooltip": { "mode": "multi", "sort": "desc" }, "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "title": "Event Ingestion by Type",
      "type": "timeseries",
      "targets": [
        { "expr": "sum(rate(analytics_events_ingested_total{job=\"analytics-service\"}[5m])) by (event_type)", "legendFormat": "{{event_type}}", "refId": "A" }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "ms", "color": { "mode": "palette-classic" } }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "id": 11,
      "options": { "tooltip": { "mode": "multi", "sort": "desc" }, "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "title": "Ingestion Processing Latency",
      "type": "timeseries",
      "targets": [
        { "expr": "histogram_quantile(0.50, sum(rate(analytics_ingestion_duration_ms_bucket{job=\"analytics-service\"}[5m])) by (le))", "legendFormat": "p50", "refId": "A" },
        { "expr": "histogram_quantile(0.95, sum(rate(analytics_ingestion_duration_ms_bucket{job=\"analytics-service\"}[5m])) by (le))", "legendFormat": "p95", "refId": "B" },
        { "expr": "histogram_quantile(0.99, sum(rate(analytics_ingestion_duration_ms_bucket{job=\"analytics-service\"}[5m])) by (le))", "legendFormat": "p99", "refId": "C" }
      ]
    },
    { "collapsed": false, "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 }, "id": 102, "title": "Queries & Exports", "type": "row" },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "ms", "color": { "mode": "palette-classic" } }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 },
      "id": 20,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "title": "Query Duration by Type",
      "type": "timeseries",
      "targets": [
        { "expr": "histogram_quantile(0.99, sum(rate(analytics_query_duration_ms_bucket{job=\"analytics-service\"}[5m])) by (le, query_type))", "legendFormat": "{{query_type}} p99", "refId": "A" }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "reqps", "color": { "mode": "palette-classic" } }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 },
      "id": 21,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "title": "HTTP Request Rate by Endpoint",
      "type": "timeseries",
      "targets": [
        { "expr": "sum(rate(http_requests_total{job=\"analytics-service\"}[5m])) by (route)", "legendFormat": "{{route}}", "refId": "A" }
      ]
    },
    { "collapsed": false, "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 }, "id": 103, "title": "System Resources", "type": "row" },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "percent", "min": 0, "max": 100 }, "overrides": [] },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 24 },
      "id": 30,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "title": "CPU Usage",
      "type": "timeseries",
      "targets": [{ "expr": "rate(process_cpu_seconds_total{job=\"analytics-service\"}[5m]) * 100", "legendFormat": "{{instance}}", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "bytes" }, "overrides": [] },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 24 },
      "id": 31,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "title": "Memory (RSS)",
      "type": "timeseries",
      "targets": [{ "expr": "process_resident_memory_bytes{job=\"analytics-service\"}", "legendFormat": "{{instance}}", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 24 },
      "id": 32,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "title": "DB Connection Pool",
      "type": "timeseries",
      "targets": [
        { "expr": "prisma_pool_connections_open{job=\"analytics-service\"}", "legendFormat": "open {{instance}}", "refId": "A" },
        { "expr": "prisma_pool_connections_idle{job=\"analytics-service\"}", "legendFormat": "idle {{instance}}", "refId": "B" }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "tags": ["lomash-wood", "analytics-service"],
  "templating": { "list": [{ "current": {}, "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "definition": "label_values(up{job=\"analytics-service\"}, instance)", "hide": 0, "includeAll": true, "multi": true, "name": "instance", "options": [], "query": "label_values(up{job=\"analytics-service\"}, instance)", "refresh": 2, "type": "query", "label": "Instance" }] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Analytics Service",
  "uid": "lomash-analytics-service",
  "version": 1,
  "weekStart": ""
}
