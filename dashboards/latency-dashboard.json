        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] }
      },
      "title": "p99 Latency — All Services",
      "type": "timeseries",
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_ms_bucket{job=\"api-gateway\"}[5m])) by (le))",          "legendFormat": "API Gateway",   "refId": "A" },
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_ms_bucket{job=\"auth-service\"}[5m])) by (le))",         "legendFormat": "Auth",          "refId": "B" },
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_ms_bucket{job=\"order-payment-service\"}[5m])) by (le))", "legendFormat": "Order/Payment", "refId": "C" },
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_ms_bucket{job=\"appointment-service\"}[5m])) by (le))",   "legendFormat": "Appointment",   "refId": "D" },
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_ms_bucket{job=\"content-service\"}[5m])) by (le))",       "legendFormat": "Content",       "refId": "E" },
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_ms_bucket{job=\"customer-service\"}[5m])) by (le))",      "legendFormat": "Customer",      "refId": "F" },
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_ms_bucket{job=\"notification-service\"}[5m])) by (le))",  "legendFormat": "Notification",  "refId": "G" },
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_ms_bucket{job=\"analytics-service\"}[5m])) by (le))",     "legendFormat": "Analytics",     "refId": "H" }
      ]
    },

    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "lineWidth": 2, "fillOpacity": 5 },
