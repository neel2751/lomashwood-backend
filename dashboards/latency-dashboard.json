      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] }
      },
      "title": "p50 Latency — All Services",
      "type": "timeseries",
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_ms_bucket{job=\"api-gateway\"}[5m])) by (le))",          "legendFormat": "API Gateway",   "refId": "A" },
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_ms_bucket{job=\"auth-service\"}[5m])) by (le))",         "legendFormat": "Auth",          "refId": "B" },
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_ms_bucket{job=\"order-payment-service\"}[5m])) by (le))", "legendFormat": "Order/Payment", "refId": "C" },
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_ms_bucket{job=\"appointment-service\"}[5m])) by (le))",   "legendFormat": "Appointment",   "refId": "D" },
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_ms_bucket{job=\"content-service\"}[5m])) by (le))",       "legendFormat": "Content",       "refId": "E" },
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_ms_bucket{job=\"customer-service\"}[5m])) by (le))",      "legendFormat": "Customer",      "refId": "F" },
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_ms_bucket{job=\"notification-service\"}[5m])) by (le))",  "legendFormat": "Notification",  "refId": "G" },
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_ms_bucket{job=\"analytics-service\"}[5m])) by (le))",     "legendFormat": "Analytics",     "refId": "H" }
      ]
    },

    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "id": 102,
      "title": "Latency by Route — Selected Service",
      "type": "row"
    },

    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "lineWidth": 2, "fillOpacity": 5 },
          "unit": "ms",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 },
      "id": 20,
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] }
      },
      "title": "p99 Latency by Route — $service",
      "type": "timeseries",
      "targets": [{
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_ms_bucket{job=~\"$service\"}[5m])) by (le, route))",
        "legendFormat": "{{route}}",
        "refId": "A"
      }]
    },

    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "unit": "ms" },
        "overrides": [
          { "matcher": { "id": "byName", "options": "p50"  }, "properties": [{ "id": "unit", "value": "ms" }] },
          { "matcher": { "id": "byName", "options": "p90"  }, "properties": [{ "id": "unit", "value": "ms" }] },
          { "matcher": { "id": "byName", "options": "p95"  }, "properties": [{ "id": "unit", "value": "ms" }] },
          { "matcher": { "id": "byName", "options": "p99"  }, "properties": [
            { "id": "unit", "value": "ms" },
            { "id": "thresholds", "value": { "mode": "absolute", "steps": [
              { "color": "green",  "value": null },
              { "color": "yellow", "value": 200  },
              { "color": "red",    "value": 500  }
            ]}},
            { "id": "custom.displayMode", "value": "color-background" }
          ]}
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 },
      "id": 21,
      "options": {
        "sortBy": [{ "displayName": "p99", "desc": true }],
        "footer": { "show": false }
      },
      "title": "Top Routes by Latency — $service (instant)",
      "type": "table",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "topk(15, histogram_quantile(0.50, sum(rate(http_request_duration_ms_bucket{job=~\"$service\"}[5m])) by (le, route)))",
          "legendFormat": "{{route}}",
          "refId": "A",
          "instant": true,
          "format": "table"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "topk(15, histogram_quantile(0.90, sum(rate(http_request_duration_ms_bucket{job=~\"$service\"}[5m])) by (le, route)))",
          "legendFormat": "{{route}}",
          "refId": "B",
          "instant": true,
          "format": "table"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "topk(15, histogram_quantile(0.99, sum(rate(http_request_duration_ms_bucket{job=~\"$service\"}[5m])) by (le, route)))",
          "legendFormat": "{{route}}",
          "refId": "C",
          "instant": true,
          "format": "table"
        }
      ],
      "transformations": [
        { "id": "merge", "options": {} },
        { "id": "organize", "options": {
          "renameByName": { "route": "Route", "Value #A": "p50", "Value #B": "p90", "Value #C": "p99" },
          "excludeByName": { "Time": true, "le": true }
        }}
      ]
    },

    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 },
      "id": 103,
      "title": "Request Duration Heatmap — API Gateway",
      "type": "row"
    },

    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": {}, "overrides": [] },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 24 },
      "id": 30,
      "options": {
        "calculate": false,
        "color": { "mode": "scheme", "scheme": "Spectral", "steps": 64 },
        "exemplars": { "color": "rgba(255,0,255,0.7)" },
        "filterValues": { "le": 1e-9 },
        "legend": { "show": true },
        "rowsFrame": { "layout": "auto" },
        "tooltip": { "show": true, "yHistogram": false },
        "yAxis": { "axisLabel": "ms", "axisWidth": 0, "decimals": 0, "reverse": false, "unit": "ms" }
      },
      "title": "Request Duration Heatmap — API Gateway",
      "type": "heatmap",
      "targets": [{
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "expr": "sum(increase(http_request_duration_ms_bucket{job=\"api-gateway\"}[$__interval])) by (le)",
        "legendFormat": "{{le}}",
        "refId": "A",
        "format": "heatmap"
      }]
    },

    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 32 },
      "id": 104,
      "title": "Database & Cache Latency",
      "type": "row"
    },

    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "lineWidth": 2, "fillOpacity": 8 },
          "unit": "ms",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 33 },
      "id": 40,
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] }
      },
      "title": "Prisma Query Latency (p99) — per Service",
      "type": "timeseries",
      "targets": [{
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "expr": "histogram_quantile(0.99, sum(rate(prisma_query_duration_ms_bucket[5m])) by (le, job))",
        "legendFormat": "{{job}}",
        "refId": "A"
      }]
    },

    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "lineWidth": 2, "fillOpacity": 8 },
          "unit": "µs",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 33 },
      "id": 41,
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] }
      },
      "title": "Redis Command Latency (p99) — by Command",
      "type": "timeseries",
      "targets": [{
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "expr": "redis_command_latency_usec{quantile=\"0.99\"}",
        "legendFormat": "{{cmd}}",
        "refId": "A"
      }]
    },

    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "lineWidth": 2, "fillOpacity": 8 },
          "unit": "ms",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 33 },
      "id": 42,
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] }
      },
      "title": "PostgreSQL Query Latency (p99)",
      "type": "timeseries",
      "targets": [{
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "expr": "topk(5, rate(pg_stat_statements_total_time[5m]) / rate(pg_stat_statements_calls[5m]))",
        "legendFormat": "{{queryid}}",
        "refId": "A"
      }]
    },

    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 41 },
      "id": 105,
      "title": "External Integration Latency",
      "type": "row"
    },

    {
