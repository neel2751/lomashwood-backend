        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "id": 1,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "API Gateway p99",
      "type": "stat",
      "targets": [{
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_ms_bucket{job=\"api-gateway\"}[5m])) by (le))",
        "legendFormat": "p99 ms",
        "refId": "A"
      }]
    },

    {
