    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "title": "Global p99 Latency Overview",
      "type": "row"
    },

    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "green",  "value": null  },
            { "color": "yellow", "value": 200   },
            { "color": "red",    "value": 500   }
          ]},
          "unit": "ms",
          "decimals": 0
