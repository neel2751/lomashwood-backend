{
  "__inputs": [{ "name": "DS_PROMETHEUS", "label": "Prometheus", "type": "datasource", "pluginId": "prometheus", "pluginName": "Prometheus" }],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "" }
  ],
  "annotations": { "list": [{ "builtIn": 1, "datasource": { "type": "grafana", "uid": "-- Grafana --" }, "enable": true, "hide": true, "iconColor": "rgba(0, 211, 255, 1)", "name": "Annotations & Alerts", "type": "dashboard" }] },
  "description": "Lomash Wood â€” Customer Service: profiles, wishlists, reviews, support tickets, loyalty points",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    { "collapsed": false, "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 }, "id": 100, "title": "Customer Overview", "type": "row" },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "id": 1,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Total Active Customers",
      "type": "stat",
      "targets": [{ "expr": "customer_active_profiles_total{job=\"customer-service\"}", "legendFormat": "customers", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "unit": "reqps" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 },
      "id": 2,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Profile Updates/min",
      "type": "stat",
      "targets": [{ "expr": "sum(rate(customer_profile_updates_total{job=\"customer-service\"}[1m]))", "legendFormat": "updates/m", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 },
      "id": 3,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Open Support Tickets",
      "type": "stat",
      "targets": [{ "expr": "customer_support_tickets_open{job=\"customer-service\"}", "legendFormat": "open tickets", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "id": 4,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Reviews Submitted (24h)",
      "type": "stat",
      "targets": [{ "expr": "sum(increase(customer_reviews_total{job=\"customer-service\"}[24h]))", "legendFormat": "reviews", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 },
      "id": 5,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Wishlist Actions (1h)",
      "type": "stat",
      "targets": [{ "expr": "sum(increase(customer_wishlist_actions_total{job=\"customer-service\"}[1h]))", "legendFormat": "actions", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 },
      "id": 6,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Loyalty Points Issued (1h)",
      "type": "stat",
      "targets": [{ "expr": "sum(increase(customer_loyalty_points_issued_total{job=\"customer-service\"}[1h]))", "legendFormat": "points", "refId": "A" }]
    },
    { "collapsed": false, "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 }, "id": 101, "title": "Customer Activity Rates", "type": "row" },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "reqps", "color": { "mode": "palette-classic" } }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "id": 10,
      "options": { "tooltip": { "mode": "multi", "sort": "desc" }, "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "title": "Customer Operations Rate",
      "type": "timeseries",
      "targets": [
        { "expr": "sum(rate(customer_profile_updates_total{job=\"customer-service\"}[5m]))", "legendFormat": "Profile Updates", "refId": "A" },
        { "expr": "sum(rate(customer_reviews_total{job=\"customer-service\"}[5m]))", "legendFormat": "Reviews", "refId": "B" },
        { "expr": "sum(rate(customer_wishlist_actions_total{job=\"customer-service\"}[5m]))", "legendFormat": "Wishlist Actions", "refId": "C" },
        { "expr": "sum(rate(customer_support_tickets_created_total{job=\"customer-service\"}[5m]))", "legendFormat": "Support Tickets", "refId": "D" }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "ms", "color": { "mode": "palette-classic" } }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "id": 11,
      "options": { "tooltip": { "mode": "multi", "sort": "desc" }, "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "title": "API Latency Percentiles",
      "type": "timeseries",
      "targets": [
        { "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_ms_bucket{job=\"customer-service\"}[5m])) by (le))", "legendFormat": "p50", "refId": "A" },
        { "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_ms_bucket{job=\"customer-service\"}[5m])) by (le))", "legendFormat": "p95", "refId": "B" },
        { "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_ms_bucket{job=\"customer-service\"}[5m])) by (le))", "legendFormat": "p99", "refId": "C" }
      ]
    },
    { "collapsed": false, "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 }, "id": 102, "title": "Support & Reviews", "type": "row" },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "short", "color": { "mode": "palette-classic" } }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 },
      "id": 20,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "title": "Support Tickets by Status",
      "type": "timeseries",
      "targets": [
        { "expr": "customer_support_tickets_total{job=\"customer-service\"}", "legendFormat": "{{status}}", "refId": "A" }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "short", "color": { "mode": "palette-classic" } }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 },
      "id": 21,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "title": "Review Rating Distribution",
      "type": "timeseries",
      "targets": [
        { "expr": "sum(rate(customer_reviews_total{job=\"customer-service\"}[5m])) by (rating)", "legendFormat": "{{rating}} stars", "refId": "A" }
      ]
    },
    { "collapsed": false, "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 }, "id": 103, "title": "System Resources", "type": "row" },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "percent", "min": 0, "max": 100 }, "overrides": [] },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 24 },
      "id": 30,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "title": "CPU Usage",
      "type": "timeseries",
      "targets": [{ "expr": "rate(process_cpu_seconds_total{job=\"customer-service\"}[5m]) * 100", "legendFormat": "{{instance}}", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "bytes" }, "overrides": [] },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 24 },
      "id": 31,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "title": "Memory (RSS)",
      "type": "timeseries",
      "targets": [{ "expr": "process_resident_memory_bytes{job=\"customer-service\"}", "legendFormat": "{{instance}}", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 10 }, "unit": "reqps" }, "overrides": [] },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 24 },
      "id": 32,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "title": "HTTP Request Rate",
      "type": "timeseries",
      "targets": [
        { "expr": "sum(rate(http_requests_total{job=\"customer-service\"}[5m])) by (status)", "legendFormat": "{{status}}", "refId": "A" }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "tags": ["lomash-wood", "customer-service"],
  "templating": { "list": [{ "current": {}, "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "definition": "label_values(up{job=\"customer-service\"}, instance)", "hide": 0, "includeAll": true, "multi": true, "name": "instance", "options": [], "query": "label_values(up{job=\"customer-service\"}, instance)", "refresh": 2, "type": "query", "label": "Instance" }] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Customer Service",
  "uid": "lomash-customer-service",
  "version": 1,
  "weekStart": ""
}
